<h4>All Followers </h4>
<ol>
<%@followers.each do |f|%>
  <li>
    Name: <%= f.dig(:Username)%>
    Email: <%= f.dig(:Email) %>
    <form action='/follows/follow/<%= f.dig(:Username) %>' method='post'>
      <button type='submit' class='btn btn-primary'>Follow</button>
    </form>
  </li>
<% end %>
</ol>