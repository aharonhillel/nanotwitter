<div class="my-3 p-3 bg-white rounded shadow-sm">
  <h6 class="border-bottom border-gray pb-2 mb-0">Following tweets</h6>
  <% @following_tweets.each do |t| %>
    <%# byebug %>
    <div class="card" style="width: 18rem;">
      <img class="card-img-top" src="..." alt="Card image cap">
      <div class="card-body">
        <strong class="d-block text-gray-dark"><a href="/users/<%= t.user.username %>"><%= t.user.username %></a></strong><h5 class="card-title">@<%= t.user.username %></h5>
        <p class="card-text"><%= t.content.to_s %></p>
        <a href="#" class="btn btn-primary">links of like, comment, retweet</a>
      </div>
    </div>
    <% end %>
    <small class="d-block text-right mt-3">
      <a href="#">Show more tweets</a>
    </small>
  </div>

  <div class="my-3 p-3 bg-white rounded shadow-sm">
    <h6 class="border-bottom border-gray pb-2 mb-0">Trending</h6>
    <% mock_user_tweets.each do |t| %>
      <div class="media text-muted pt-3">
        <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray">
          <strong class="d-block text-gray-dark">@<%= mock_user.username %></strong>
          <%= t.content.to_s %>
        </p>
      </div>
    <% end %>
    <small class="d-block text-right mt-3">
      <a href="#">Show more trending tweets</a>
    </small>
  </div>
</main>
